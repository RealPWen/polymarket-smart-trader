# Polymarket Configuration Template
# Copy this file to .env and fill in your details

POLYMARKET_PRIVATE_KEY=your_private_key_here
POLYMARKET_FUNDER_ADDRESS=your_proxy_wallet_address_here
POLYMARKET_SIGNATURE_TYPE=1
MIN_REQUIRED_USDC=5.0

# Email Alert Configuration (Optional)
# If not configured, email alerts will be disabled
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
EMAIL_RECEIVER=receiver_email@example.com
